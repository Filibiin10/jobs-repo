{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1", // Corrected to redirect to the matching route
      "methods": ["GET", "POST", "PUT", "DELETE"], // Specify methods for the route
      "headers": {
        "Access-Control-Allow-Origin": "https://your-frontend-domain.com", // Set to your frontend URL
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
  
